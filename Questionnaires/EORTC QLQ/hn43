  hn43_pain:
    "#type": computed_twig
    "#title": hn43_pain
    "#display_on": view
    "#template": >-
      {% set scores =
      [data.hn43_q31,data.hn43_q32,data.hn43_q33,data.hn43_q34] %} 

      {% set full_scores = [] %} 

      {% for v in scores|filter(v => v is not empty) -%} 

      {% set full_scores = full_scores|merge([v]) %} 

      {% endfor %}

      {% if full_scores|length > 0 %} 

      {% set raw_score = full_scores|reduce((carry,v) => carry+v) %} 

      {% set raw_score = raw_score/(full_scores|length)  %}

      {% set score = ((raw_score-1)/3)*100  %}

      {{score}}  

      {% else %} 

      {% set score = '' %} 

      {{ score }} 

      {% endif %}
    "#whitespace": spaceless
    "#store": true
